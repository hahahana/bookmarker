{
  "name": "Bookmarker",
  "version": "0.1",
  "description": "Sort all the things!",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "Bookmarker",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}